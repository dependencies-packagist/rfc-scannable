{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "rfc/scannable",
    "description": "Implement a scanner that can scan by namespace or path, etc.",
    "keywords": [
        "Scan",
        "Scanner",
        "Scannable"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/rfc-scannable",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/collections": ">=8.0.0",
        "illuminate/support": ">=8.0.0",
        "reflective/reflection": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Rfc\\Scannable\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
